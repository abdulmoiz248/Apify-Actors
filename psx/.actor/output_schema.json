{
    "actorOutputSchemaVersion": 1,
    "title": "PSX Company Data",
    "description": "Scraped data from Pakistan Stock Exchange company pages",
    "properties": {
        "symbol": {
            "type": "string",
            "title": "Stock Symbol",
            "description": "Company stock symbol on PSX",
            "template": "{{symbol}}"
        },
        "url": {
            "type": "string",
            "title": "Company URL",
            "description": "PSX company profile page URL",
            "template": "{{url}}"
        },
        "businessDescription": {
            "type": "string",
            "title": "Business Description",
            "description": "Company business overview",
            "template": "{{companyProfile.businessDescription}}"
        },
        "address": {
            "type": "string",
            "title": "Address",
            "description": "Company registered address",
            "template": "{{companyProfile.address}}"
        },
        "website": {
            "type": "string",
            "title": "Website",
            "description": "Company website URL",
            "template": "{{companyProfile.website}}"
        },
        "marketCap": {
            "type": "string",
            "title": "Market Capitalization",
            "description": "Total market capitalization",
            "template": "{{equityProfile.marketCap}}"
        },
        "totalShares": {
            "type": "string",
            "title": "Total Shares",
            "description": "Total number of shares",
            "template": "{{equityProfile.totalShares}}"
        },
        "close": {
            "type": "string",
            "title": "Close Price",
            "description": "Last closing price",
            "template": "{{marketData.close}}"
        },
        "volume": {
            "type": "string",
            "title": "Volume",
            "description": "Trading volume",
            "template": "{{marketData.volume}}"
        },
        "peRatio": {
            "type": "string",
            "title": "P/E Ratio",
            "description": "Price to Earnings Ratio (TTM)",
            "template": "{{marketData.pe_ratio_ttm_}}"
        }
    }
}
